<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ApplicationBackgroundColor">#FFFAFAFA</Color>
    <SolidColorBrush x:Key="ApplicationBackgroundBrush" Color="{StaticResource ApplicationBackgroundColor}" />

    <!--  Colors  -->
    <Color x:Key="PrimaryColor">#F5F5F5</Color>
    <Color x:Key="SecondaryColor">#E0E0E0</Color>
    <Color x:Key="PrimaryTextColor">#000000</Color>
    <Color x:Key="SecondaryTextColor">#757575</Color>
    <Color x:Key="DividerColor">#BDBDBD</Color>

    <Color x:Key="SnowflakeGradientTop">#2E4053</Color>
    <Color x:Key="SnowflakeGradientBottom">#CCCCCC</Color>

    <Color x:Key="OverlayGradientTop">#DDD</Color>
    <Color x:Key="OverlayGradientBottom">#EEE</Color>

    <Color x:Key="TextFillColorPrimary">#E4000000</Color>
    <Color x:Key="TextFillColorSecondary">#9E000000</Color>
    <Color x:Key="TextEditorBackgroundColor">#FFFFFF</Color>
    <Color x:Key="TextEditorForegroundColor">#000000</Color>
    <Color x:Key="TextEditorSelectionColor">#ADD6FF</Color>

    <Color x:Key="LineNumbersForegroundColor">#237893</Color>
    <Color x:Key="ListViewItemBackgroundColor">#E1E1E1</Color>
    <Color x:Key="HoverBorderColor">#FFBDBDBD</Color>

    <Color x:Key="ButtonBackgroundColorBase">#DDDDDD</Color>
    <Color x:Key="ButtonForegroundColorBase">#333333</Color>
    <Color x:Key="ButtonHoverBackgroundColorBase">#EEEEEE</Color>
    <Color x:Key="ButtonPressedBackgroundColorBase">#CCCCCC</Color>

    <Color x:Key="DisabledBackgroundColor">#B0B0B0</Color>
    <Color x:Key="DisabledForegroundColor">#F0F0F0</Color>

    <Color x:Key="LoadingPlaceholderTop">#FFFFFF</Color>
    <Color x:Key="LoadingPlaceholderBottom">#5c7bd9</Color>
    <Color x:Key="LoadingPlaceholderBackgroundTop">#FAFAFA</Color>
    <Color x:Key="LoadingPlaceholderBackgroundBottom">#E0E0E0</Color>

    <Color x:Key="FadeOutBackgroundColor">#BDBDBD</Color>

    <Color x:Key="ChatInputBoxColor">#f3f4f6</Color>

    <Color x:Key="BuildRunBackgroundColor">#ffffff</Color>
    <Color x:Key="BuildRunCanvasBackgroundColor">#f4f4f5</Color>
    <Color x:Key="BuildButtonBackgroundColor">#FAFAFA</Color>
    <Color x:Key="BuildButtonForegroundColor">#333333</Color>
    <Color x:Key="BuildButtonHoverBackgroundColor">#f4f4f5</Color>
    <Color x:Key="BuildButtonPressedBackgroundColor">#E2E2E3</Color>
    <Color x:Key="BuildButtonDisabledBackgroundColor">#EBEBEC</Color>

    <Color x:Key="FloatingToolbarBackgroundColor">#f5f5f5</Color>

    <Color x:Key="HistoryNotificationsBackgroundColor">#F0F0F0</Color>

    <Color x:Key="IconLinearGradientBrushTop">#6a11cb</Color>
    <Color x:Key="IconLinearGradientBrushBottom">#2575fc</Color>

    <Color x:Key="ModuleCardPrimaryColor">#ffffff</Color>
    <Color x:Key="ModuleCardSecondaryColor">#f4f4f5</Color>

    <Color x:Key="PopupBorderCornerRadiusColor">#c7c7c7</Color>
    <Color x:Key="PopupBorderColor">#ffffff</Color>

    <Color x:Key="RoundedButtonBackgroundMouseOverColor">#E5E7EB</Color>
    <Color x:Key="RoundedButtonBackgroundPressedColor">#D1D5DB</Color>

    <Color x:Key="PrismaTalkBorderColor">#f3f3f3</Color>
    <Color x:Key="PrismaTalkBackgroundColor">#ffff</Color>

    <!--  Brushes  -->
    <SolidColorBrush x:Key="GroupBoxBackgroundColor" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="GroupBoxForegroundColor" Color="{StaticResource PrimaryTextColor}" />
    <SolidColorBrush x:Key="ListViewItemBackground" Color="{StaticResource ListViewItemBackgroundColor}" />

    <SolidColorBrush x:Key="MasBorderBrush" Color="{StaticResource DividerColor}" />
    <SolidColorBrush x:Key="MasDividerBrush" Color="{StaticResource DividerColor}" />
    <SolidColorBrush x:Key="HoverBorderBrush" Color="{StaticResource HoverBorderColor}" />

    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="{StaticResource ButtonBackgroundColorBase}" />
    <SolidColorBrush x:Key="ButtonForegroundColor" Color="{StaticResource ButtonForegroundColorBase}" />
    <SolidColorBrush x:Key="ButtonHoverBackgroundColor" Color="{StaticResource ButtonHoverBackgroundColorBase}" />
    <SolidColorBrush x:Key="ButtonPressedBackgroundColor" Color="{StaticResource ButtonPressedBackgroundColorBase}" />

    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="{StaticResource PrimaryColor}" />

    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="{StaticResource PrimaryTextColor}" />
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="{StaticResource PrimaryTextColor}" />

    <SolidColorBrush x:Key="TextFillColorPrimaryBrush" Color="{StaticResource TextFillColorPrimary}" />
    <SolidColorBrush x:Key="TextFillColorSecondaryBrush" Color="{StaticResource TextFillColorSecondary}" />
    <SolidColorBrush x:Key="TextEditorBackgroundBrush" Color="{StaticResource TextEditorBackgroundColor}" />
    <SolidColorBrush x:Key="TextEditorForegroundBrush" Color="{StaticResource TextEditorForegroundColor}" />
    <SolidColorBrush x:Key="TextEditorSelectionBrush" Color="{StaticResource TextEditorSelectionColor}" />
    <SolidColorBrush x:Key="LineNumbersForegroundBrush" Color="{StaticResource LineNumbersForegroundColor}" />

    <LinearGradientBrush x:Key="MaskGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.0" Color="{StaticResource OverlayGradientTop}" />
        <GradientStop Offset="1.0" Color="{StaticResource OverlayGradientBottom}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LoadingPlaceholderBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.0" Color="{StaticResource LoadingPlaceholderTop}" />
        <GradientStop Offset="1.0" Color="{StaticResource LoadingPlaceholderBottom}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="LoadingPlaceholderBackgroundBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.0" Color="{StaticResource LoadingPlaceholderBackgroundTop}" />
        <GradientStop Offset="1.0" Color="{StaticResource LoadingPlaceholderBackgroundBottom}" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="{StaticResource DisabledBackgroundColor}" />
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="{StaticResource DisabledForegroundColor}" />

    <SolidColorBrush x:Key="FadeOutBackgroundBrush" Color="{StaticResource FadeOutBackgroundColor}" />

    <SolidColorBrush x:Key="ChatInputBoxBrush" Color="{StaticResource ChatInputBoxColor}" />

    <SolidColorBrush x:Key="BuildRunBackgroundBrush" Color="{StaticResource BuildRunBackgroundColor}" />
    <SolidColorBrush x:Key="BuildRunCanvasBackgroundBrush" Color="{StaticResource BuildRunCanvasBackgroundColor}" />
    <SolidColorBrush x:Key="BuildButtonBackgroundBrush" Color="{StaticResource BuildButtonBackgroundColor}" />
    <SolidColorBrush x:Key="BuildButtonForegroundBrush" Color="{StaticResource BuildButtonForegroundColor}" />
    <SolidColorBrush x:Key="BuildButtonHoverBackgroundBrush" Color="{StaticResource BuildButtonHoverBackgroundColor}" />
    <SolidColorBrush x:Key="BuildButtonPressedBackgroundBrush" Color="{StaticResource BuildButtonPressedBackgroundColor}" />
    <SolidColorBrush x:Key="BuildButtonDisabledBackgroundBrush" Color="{StaticResource BuildButtonDisabledBackgroundColor}" />

    <SolidColorBrush x:Key="FloatingToolbarBackgroundBrush" Color="{StaticResource FloatingToolbarBackgroundColor}" />

    <SolidColorBrush x:Key="HistoryNotificationsBackgroundBrush" Color="{StaticResource HistoryNotificationsBackgroundColor}" />

    <LinearGradientBrush x:Key="IconLinearGradientBrushBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.3" Color="{StaticResource IconLinearGradientBrushTop}" />
        <GradientStop Offset="1.0" Color="{StaticResource IconLinearGradientBrushBottom}" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ModuleCardPrimaryBrush" Color="{StaticResource ModuleCardPrimaryColor}" />
    <SolidColorBrush x:Key="ModuleCardSecondaryBrush" Color="{StaticResource ModuleCardSecondaryColor}" />

    <SolidColorBrush x:Key="PopupBorderCornerRadiusBrush" Color="{StaticResource PopupBorderCornerRadiusColor}" />
    <SolidColorBrush x:Key="PopupBorderBrush" Color="{StaticResource PopupBorderColor}" />

    <SolidColorBrush x:Key="RoundedButtonBackgroundMouseOverBrush" Color="{StaticResource RoundedButtonBackgroundMouseOverColor}" />
    <SolidColorBrush x:Key="RoundedButtonBackgroundPressedBrush" Color="{StaticResource RoundedButtonBackgroundPressedColor}" />

    <SolidColorBrush x:Key="PrismaTalkBorderBrush" Color="{StaticResource PrismaTalkBorderColor}" />
    <SolidColorBrush x:Key="PrismaTalkBackgroundBrush" Color="{StaticResource PrismaTalkBackgroundColor}" />
</ResourceDictionary>
